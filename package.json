{
  "name": "tpl-vue-component",
  "version": "1.0.0",
  "description": "A code repository template for building a vue component library",
  "scripts": {
    "dev": "npm run playground",
    "playground": "pnpm -C playground dev",
    "build:watch": "pnpm -C build watch",
    "build": "pnpm -C build build",
    "build:playground": "pnpm -C build build &&  pnpm -C playground build",
    "build:theme":"pnpm -C build build:theme",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vue component",
    "vue template",
    "vue plugin",
    "vite plugin"
  ],
  "author": "liar0320",
  "license": "ISC",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "semantic-release": "^20.1.0",
    "typescript": "^4.9.5",
    "vue-eslint-parser": "^9.1.0"
  }
}
